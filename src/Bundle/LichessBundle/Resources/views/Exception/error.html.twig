{% extends "LichessBundle:Exception:base.html.twig" %}
{% block error_title %}{{ 'Server error'|trans }} ({{ status_code }}: {{ status_text }}){% endblock %}
{% block error_text %}{{ "Lichess was unable to process your request. This may be due to maintenance. Please try again soon, or contact me if the problem persists:"|trans }} <a class="js_email">email</a>{% endblock %}
{% block error_next %}<a class="lichess_new_game" href="{{ url|default(path("lichess_homepage")) }}">{{ 'Try again'|trans }}</a>{% endblock %}
{% block top_security %}
<div id="nb_connected_players" class="nb_connected_players none">
  {{ "%nb% connected players"|trans({"%nb%": "<strong>0</strong>/"})|raw }}
</div>
{% endblock %}
